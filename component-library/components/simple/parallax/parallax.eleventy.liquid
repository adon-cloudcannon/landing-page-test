<style>
    .c-simple-parallax {
        position:absolute;
        right:0;
        width:1000px;
        display:grid;
        grid-template-columns: repeat(8, 1fr);
        grid-template-rows: repeat(8, 1fr);
        gap:1rem;
        z-index:1;
        transform:rotate(-18deg);
        left:50%;
        transform-origin: top left;
    }

    .c-simple-parallax img{
        border-radius:8px;
        box-shadow: 0px 4.135px 16.541px 0px rgba(0, 0, 0, 0.20);
    }

    .c-simple-parallax img:nth-of-type(1){
        grid-column: 1 / 2;
        grid-row: 2 / 4;
    }
    .c-simple-parallax img:nth-of-type(2){
        grid-column: 2 / 3;
        grid-row: 1 / 3;
    }
    .c-simple-parallax img:nth-of-type(3){
        grid-column: 3 / 4;
        grid-row: 2 / 4;
    }
    .c-simple-parallax img:nth-of-type(4){
        grid-column: 4 / 5;
        grid-row: 1 / 3;
    }
    .c-simple-parallax img:nth-of-type(5){
        grid-column: 5 / 6;
        grid-row: 2 / 4;
    }
    .c-simple-parallax img:nth-of-type(6){
        grid-column: 6 / 7;
        grid-row: 1 / 3;
    }
    .c-simple-parallax img:nth-of-type(7){
        grid-column: 7 / 8;
        grid-row: 2 / 4;
    }
    .c-simple-parallax img:nth-of-type(8){
        grid-column: 8 / 9;
        grid-row: 1 / 3;
    }


    .c-simple-parallax img:nth-of-type(9){
        grid-column: 1 / 2;
        grid-row: 4 / 6;
    }
    .c-simple-parallax img:nth-of-type(10){
        grid-column: 2 / 3;
        grid-row: 3 / 5;
    }
    .c-simple-parallax img:nth-of-type(11){
        grid-column: 3 / 4;
        grid-row: 4 / 6;
    }
    .c-simple-parallax img:nth-of-type(12){
        grid-column: 4 / 5;
        grid-row: 3 / 5;
    }
    .c-simple-parallax img:nth-of-type(13){
        grid-column: 5 / 6;
        grid-row: 4 / 6;
    }
    .c-simple-parallax img:nth-of-type(14){
        grid-column: 6 / 7;
        grid-row: 3 / 5;
    }
    .c-simple-parallax img:nth-of-type(15){
        grid-column: 7 / 8;
        grid-row: 4 / 6;
    }
    .c-simple-parallax img:nth-of-type(16){
        grid-column: 8 / 9;
        grid-row: 3 / 5;
    }


    .c-simple-parallax img:nth-of-type(17){
        grid-column: 1 / 2;
        grid-row: 6 / 8;
    }
    .c-simple-parallax img:nth-of-type(18){
        grid-column: 2 / 3;
        grid-row: 5 / 7;
    }
    .c-simple-parallax img:nth-of-type(19){
        grid-column: 3 / 4;
        grid-row: 6 / 8;
    }
    .c-simple-parallax img:nth-of-type(20){
        grid-column: 4 / 5;
        grid-row: 5 / 7;
    }
    .c-simple-parallax img:nth-of-type(21){
        grid-column: 5 / 6;
        grid-row: 6 / 8;
    }
    .c-simple-parallax img:nth-of-type(22){
        grid-column: 6 / 7;
        grid-row: 5 / 7;
    }
    .c-simple-parallax img:nth-of-type(23){
        grid-column: 7 / 8;
        grid-row: 6 / 8;
    }
    .c-simple-parallax img:nth-of-type(24){
        grid-column: 8 / 9;
        grid-row: 5 / 7;
    }
</style>

